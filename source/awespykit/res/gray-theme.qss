/* Note: Please save with utf-8 encoding */
/* Theme Name: 浅灰主题 */

/* 主题名必须在第二行且严格遵守以上示例格式，主题名中可以有空格 */
/*
The theme name must be on the second line and strictly follow
the example format above, with Spaces allowed in the theme name.
*/

/* 所有控件的统一背景色和字体 */
* {
    font-family: "Microsoft YaHei UI";
    font-size: 9pt;
    color: #1A1B1B;
    background-color: #F3F3F3;
}

/* 无效 Qt 样式表，由本程序解析，需单独写 */
QPlaceHolderText {
    color: #ADB4B4;
}

/* 所有控件基本样式 */
/* 按钮基本样式 */
QPushButton {
    border: 2px solid #D9D9D9;
    border-radius: 4px;
    background-color: #FBFBFB;
    padding: 2px 16px;
}
QPushButton:default {
    border-color: #DDDE3F;
}
QPushButton:hover {
    border-color: #00ACFF;
    background-color: #EAEAEA;
}
QPushButton:pressed {
    background-color: #DADADA;
}
QPushButton:disabled {
    color: #ABABAB;
    background-color: #E6E6E6;
}

/* 单行输入框基本样式 */
QLineEdit {
    border: 2px solid #D9D9D9;
    padding: 1px 1px;
    background-color: #FBFBFB;
    border-radius: 4px;
}
QLineEdit:hover {
    border-color: #ABABAB;
    background-color: #FFFFFF;
}
QLineEdit:focus {
    border-color: #00ACFF;
    background-color: #FFFFFF;
}
QLineEdit:read-only {
    border: 2px solid #EAEAEA;
    background-color: #FBFBFB;
}
QLineEdit:disabled {
    color: #ABABAB;
    border: 2px solid #EAEAEA;
    background-color: #EDEDED;
}

/* 多行富文本输入框基本样式 */
QTextEdit {
    border: 2px solid #D9D9D9;
    background-color: #FBFBFB;
    border-radius: 4px;
}
QTextEdit:hover {
    border-color: #ABABAB;
    background-color: #FFFFFF;
}
QTextEdit:focus {
    border-color: #00ACFF;
    background-color: #FFFFFF;
}
QTextEdit[readOnly=true] {
    border: 2px solid #EAEAEA;
    background-color: #FBFBFB;
}
QTextEdit:disabled {
    color: #ABABAB;
    border: 2px solid #EAEAEA;
    background-color: #EDEDED;
}

/* 多行文本输入框基本样式 */
QPlainTextEdit {
    border: 2px solid #D9D9D9;
    background-color: #FBFBFB;
    border-radius: 4px;
}
QPlainTextEdit:hover {
    border-color: #ABABAB;
    background-color: #FFFFFF;
}
QPlainTextEdit:focus {
    border-color: #00ACFF;
    background-color: #FFFFFF;
}
QPlainTextEdit[readOnly=true] {
    border: 2px solid #EAEAEA;
    background-color: #FBFBFB;
}
QPlainTextEdit:disabled {
    color: #ABABAB;
    border: 2px solid #EAEAEA;
    background-color: #EDEDED;
}

/* 弹出型菜单样式 */
QMenu {
    border: 2px solid #D1D1D1;
    border-radius: 2px;
    padding: 4px;
    background-color: #EEEEEE;
}
QMenu::item {
    border-radius: 2px;
    padding: 4px 8px;
}
QMenu::item:disabled {
    color: #ABABAB;
}
QMenu::icon {
    padding-left: 8px;
}
QMenu::item:selected {
    background-color: #E0E0E0;
}
QMenu::item[objectName="settings_menu"] {
    border-radius: 2px;
    padding: 4px 16px 4px 8px;
}
QMenu[objectName="envlst_menu"], QMenu[objectName="pkginfo_menu"] {
    padding: 4px 8px;
}

/* 列表控件样式 */
QListView {
    outline-style: none;
    border: 2px solid #D9D9D9;
    border-radius: 4px;
    background-color: #FFFFFF;
}
QListView::item {
    padding: 1px 0;
    color: #1A1B1B;
}
QListView::item:hover {
    background-color: #EAEAEA;
}
QListView::item:selected {
    background-color: #EAEAEA;
}
QListView::item:disabled {
    color: #ABABAB;
}

/* 表格控件样式 */
/* 表头区域 */
QHeaderView::section {
    background-color: transparent;
    border-left: 0px solid transparent;
    border-top: 0px solid transparent;
    border-right: 1px solid #D8D8D8;
    border-bottom: 1px solid #D8D8D8;
}
QHeaderView::section:hover {
    background-color: #E0E0E0;
}
/* 表格区域 */
QTableView {
    outline-style: none;
    border: 2px solid #D9D9D9;
    border-radius: 4px;
    color: #1A1B1B;
    background-color: #FFFFFF;
}
QTableView QTableCornerButton::section {
    background-color: #F3F3F3;
    border-left: 0px solid transparent;
    border-top: 0px solid transparent;
    border-right: 1px solid #D8D8D8;
    border-bottom: 1px solid #D8D8D8;
}
/* 以下两项也会被本程序解析，需单独写 */
QTableView::item {
    color: #1A1B1B;
    background-color: transparent;
}
QTableView::item:selected {
    color: #1A1B1B;
    background-color: #E0E0E0;
}

/* 下拉框样式 */
QComboBox {
    border: 2px solid #D9D9D9;
    border-radius: 4px;
    background-color: #FBFBFB;
    padding: 1px;
}
QComboBox:hover {
    border-color: #00ACFF;
    background-color: #EAEAEA;
}
QComboBox:focus {
    border-color: #00ACFF;
}
QComboBox:disabled {
    color: #ABABAB;
    background-color: #E6E6E6;
}
QComboBox::down-arrow:disabled {
    image: none;
}
QComboBox::drop-down {
	border: none;
}
QComboBox::down-arrow {
    width: 0.8em;
    height: 0.8em;
    image: url(:/down_arrow_dark.png);
}
QComboBox::down-arrow:on {
    width: 0.8em;
    height: 0.8em;
    image: url(:/up_arrow_dark.png);
}
QComboBox QAbstractItemView
{
    outline: 0px solid #FFFFFF;
    border: 2px solid #00ACFF;
    background-color: #FFFFFF;
    selection-background-color: #EAEAEA;
    selection-color: #1A1B1B;
}

/* 滚动条样式 */
QScrollBar {
    background-color: #FFFFFF;
    width: 6px;
}
QScrollBar::handle {
    background-color: #E6E6E6;
}
QScrollBar::handle:hover {
    background-color: #D0D0D0;
}

/* 标签页控件相关 */
QTabBar::scroller {
    border: 2px solid #D9D9D9;
}
QTabBar QToolButton {
    border-radius: 4px;
    border: 2px solid #D9D9D9;
}
QTabBar QToolButton:hover {
    background-color: #E0E0E0;
}

/* 悬浮提示样式 */
QToolTip {
    border: 2px solid #D1D4D9;
    background-color: #F3F3F3;
    padding: 4px;
}

/* 具体控件样式 */

/* 主界面 6 个按钮的样式 */
#uiPushButton_pkg_mgr, #uiPushButton_pyi_tool, #uiPushButton_index_mgr, #uiPushButton_pkg_dload,
#uiPushButton_cloudfunction, #uiPushButton_settings {
    border: none;
    border-radius: 4px;
    background-color: #F3F3F3;
    padding: 4px 4px;
}
#uiPushButton_pkg_mgr:hover, #uiPushButton_pyi_tool:hover, #uiPushButton_index_mgr:hover,
#uiPushButton_cloudfunction:hover, #uiPushButton_pkg_dload:hover, #uiPushButton_settings:hover {
    border-radius:4px;
    background-color: #C0C0C0;
}
#uiPushButton_pkg_mgr:pressed, #uiPushButton_pyi_tool:pressed, #uiPushButton_index_mgr:pressed,
#uiPushButton_cloudfunction:pressed, #uiPushButton_pkg_dload:pressed, #uiPushButton_settings:pressed {
    border-radius:4px;
    background-color: #A0A0A0;
}
#uiPushButton_pkg_mgr::menu-indicator, #uiPushButton_pyi_tool::menu-indicator, #uiPushButton_cloudfunction::menu-indicator,
#uiPushButton_index_mgr::menu-indicator, #uiPushButton_pkg_dload::menu-indicator, #uiPushButton_settings::menu-indicator {
    image:none;
}

/* 包管理器器删除环境按钮 */
#uiPushButton_delselected {
    border-color: #B62A8F;
}
#uiPushButton_delselected:hover {
    color: #FFFFFF;
    background-color: #B62A8F;
}
#uiPushButton_delselected:pressed {
    color: #FFFFFF;
    background-color: #902171;
}

/* 包管理器器安装界面开始安装按钮 */
#uiPushButton_do_install {
    border-color: #007A3B;
}
#uiPushButton_do_install:hover {
    color: #FFFFFF;
    background-color: #007A3B;
}
#uiPushButton_do_install:pressed {
    color: #FFFFFF;
    background-color: #005429;
}

/* 包管理器安装界面的提示字符 */
#uiLabel_pkginstall_tips {
    font-size: 8pt;
    font-family: 'Consolas', 'Microsoft YaHei UI', 'Monospace';
    font-weight: normal;
}

/* 包管理器右下角的运行提示字符 */
#uiLabel_loading_tip {
    font-size: 8pt;
}

/* 镜像源管理工具的启用镜像源按钮 */
#uiPushButton_set_index {
    border-color: #007A3B;
}
#uiPushButton_set_index:hover {
    color: #FFFFFF;
    background-color: #007A3B;
}
#uiPushButton_set_index:pressed {
    color: #FFFFFF;
    background-color: #005429;
}

/* 镜像源管理工具的删除镜像源按钮 */
#uiPushButton_delete_url {
    border-color: #B62A8F;
}
#uiPushButton_delete_url:hover {
    color: #FFFFFF;
    background-color: #B62A8F;
}
#uiPushButton_delete_url:pressed {
    color: #FFFFFF;
    background-color: #902171;
}

/* 程序打包工具环境检查按钮 */
#uiPushButton_check_imports {
    margin-bottom: 4px;
    border-color: #8685EF;
}
#uiPushButton_check_imports:hover {
    color: #FFFFFF;
    background-color: #8685EF;
}
#uiPushButton_check_imports:pressed {
    color: #FFFFFF;
    background-color: #7170C9;
}

/* 程序打包工具清空数据按钮 */
#uiPushButton_clear_data {
    margin-bottom: 4px;
    border-color: #F85E00;
}
#uiPushButton_clear_data:hover {
    color: #FFFFFF;
    background-color: #F85E00;
}
#uiPushButton_clear_data:pressed {
    color: #FFFFFF;
    background-color: #D25000;
}

/* 程序打包工具开始打包按钮 */
#uiPushButton_start_packing {
    margin-bottom: 4px;
    border-color: #008B69;
}
#uiPushButton_start_packing:hover {
    color: #FFFFFF;
    background-color: #008B69;
}
#uiPushButton_start_packing:pressed {
    color: #FFFFFF;
    background-color: #00654C;
}

/* 云函数打包工具开始按钮 */
#uiPushButton_start_scfpacking {
    border-color: #008B69;
}
#uiPushButton_start_scfpacking:hover {
    color: #FFFFFF;
    background-color: #008B69;
}
#uiPushButton_start_scfpacking:pressed {
    color: #FFFFFF;
    background-color: #00654C;
}

/* 云函数打包工具删除按钮 */
#uiPushButton_delete_line, #uiPushButton_del_scfconfig {
    border-color: #B62A8F;
}
#uiPushButton_delete_line:hover, #uiPushButton_del_scfconfig:hover {
    color: #FFFFFF;
    background-color: #B62A8F;
}
#uiPushButton_delete_line:pressed, #uiPushButton_del_scfconfig:pressed {
    color: #FFFFFF;
    background-color: #902171;
}

/* 云函数打包工具的超链接 */
#uiLabel_whatis_cloudfunction, #uiLabel_set_excludes {
    color: blue;
    font-size: 8pt;
}

/* 云函数打包工具排除目录和文件窗口按钮 */
#uiPushButton_add_filepath, #uiPushButton_add_dirpath,
#uiPushButton_del_line, #uiPushButton_clear_items, #uiPushButton_new_line {
    padding: 2px;
    background-color: #F3F3F3;
    border: 0px solid transparent;
}
#uiPushButton_add_filepath:hover, #uiPushButton_add_dirpath:hover,
#uiPushButton_del_line:hover, #uiPushButton_clear_items:hover, #uiPushButton_new_line:hover {
    border: 1px solid #00ACFF;
    background-color: #EAEAEA;
}
#uiPushButton_add_filepath:pressed, #uiPushButton_add_dirpath:pressed,
#uiPushButton_del_line:pressed, #uiPushButton_clear_items:pressed, #uiPushButton_new_line:pressed {
    background-color: #DADADA;
}
/* 云函数打包工具排除文件窗口的提示 */
#uiLabel_exclude_tips {
    font-size: 8pt;
    color: #909090;
}

/* 程序打包工具命令输出窗口 */
#uiTextEdit_packtool_logging {
    font-size: 10pt;
    font-family: 'Consolas', 'Monospace', 'Microsoft YaHei UI';
    font-weight: normal;
}

/* 程序打包工具右上角几个版本信息标签及包管理器安装界面环境标签 */
#uiLabel_python_info, #uiLabel_venv_info, #uiLabel_packtool_info,
#uiLabel_platform_info, #uiLabel_target_environment {
    border-radius: 4px;
    border: 2px solid #D0D0D0;
    padding: 1px 1px;
}

/* 程序打包工具右下角的运行提示字符 */
#uiLabel_running_tip {
    font-size: 8pt;
}

/* 程序打包工具的工具设置标签页的删除按钮 */
#uiPushButton_delete_config {
    margin-top: 12px;
    border-color: #B62A8F;
}
#uiPushButton_delete_config:hover {
    color: #FFFFFF;
    background-color: #B62A8F;
}
#uiPushButton_delete_config:pressed {
    color: #FFFFFF;
    background-color: #902171;
}

/* 命令通用输出窗口 */
#uiPlainTextEdit_output {
    font-size: 10pt;
    font-family: 'Consolas', 'Monospace', 'Microsoft YaHei UI';
    font-weight: normal;
}

/* 模块安装包下载器的下载提示字符 */
#uiLabel_pkgdownload_tips {
    font-size: 8pt;
    font-family: 'Consolas', 'Microsoft YaHei UI', 'Monospace';
    font-weight: normal;
}

/* 模块安装包下载器的下载列表按钮 */
#uiPushButton_show_dllist {
    margin-bottom: 4px;
}

/* 模块安装包下载器的开始下载按钮 */
#uiPushButton_start_download {
    margin-bottom: 4px;
    border-color: #008B69;
}
#uiPushButton_start_download:hover {
    color: #FFFFFF;
    background-color: #008B69;
}
#uiPushButton_start_download:pressed {
    color: #FFFFFF;
    background-color: #00654C;
}

/* 关于窗口的相关样式 */
QMainWindow[objectName="about_window"] QLabel {
    font-size: 9pt;
}
#uiGroupBox_app_name {
    font-size: 22pt;
    padding-top: 8px;
    margin-top: 22px;
}
#uiGroupBox_app_name::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    left: 16px;
}
#uiLabel_app_description {
    font-size: 9pt;
}
#uiLabel_app_version, #uiLabel_license, #uiLabel_source_github,
#uiLabel_source_gitee, #uiLabel_issue_github, #uiLabel_issue_gitee {
    color: blue;
}
#uiPlainTextEdit_open_packages {
    font-size: 9pt;
}
